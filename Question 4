
%matplotlib inline #only include this if you are using jupyter

import matplotlib.pyplot as plt
import pandas as pd
water_file = pd.read_csv('CO-OPS__8729108__wl.csv') #make sure to upload the data file to jupyter

wl = water_file[' Water Level']
time = water_file['Date Time']

fig = plt.figure()
plt.plot(time, wl, '-')
plt.tick_params(
    axis='x',          # changes apply to the x-axis
    which='both',      # both major and minor ticks are affected
    bottom=False,      # ticks along the bottom edge are off
    top=False,         # ticks along the top edge are off
    labelbottom=False)
plt.axis([0, 750, 0, 7])
plt.show()
